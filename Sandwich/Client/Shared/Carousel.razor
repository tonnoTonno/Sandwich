#burger:checked ~ [for="burger"] .top_bun { border-top-left-radius: 32px; border-top-right-radius: 32px; }
#burger:checked ~ [for="burger"] .cheese { height: 6px; margin-top: 20px; background: #f6b93b; }
#burger:checked ~ [for="burger"] .cheese:after { border-top-width: 5px; }
#burger:checked ~ [for="burger"] .cheese:before { top: 6px; height: 6px; }
#burger:checked ~ [for="burger"] .bot_bun { border-bottom-left-radius: 32px; border-bottom-right-radius: 32px; margin-top: 32px; }

@@-webkit-keyframes jello-vertical {
  0% {
    -webkit-transform: scale3d(1, 1, 1);
            transform: scale3d(1, 1, 1);
  }
