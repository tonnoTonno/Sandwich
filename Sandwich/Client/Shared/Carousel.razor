@page "/car"
<style>
body, html { overflow: hidden; }

    body {
       display: flex;
        justify-content: center; 
        align-items: center; 
       position: absolute; 
       width: 100%; 
       height: 100%; 
       margin: 0; }

#burger { position: absolute; left: 20px; top: 20px; }

label[for="burger"] { cursor: pointer 16 0,auto; position: relative; width: 193px; height: 193px; display: block; margin: auto; overflow: hidden; } /*contenitore burger*/
label[for="burger"] > span { position: absolute; left: 0; top: 0; width: 100%; height: 10px; background-color: #FFF; display: block; } /*barre menu*/
label[for="burger"], label[for="burger"] * { transition: all .3s ease-in-out; } 

.cheese { margin-top: 26px; }
.cheese:before, .cheese:after { content: ""; position: absolute; }
.cheese:before { height: 0; width: 100%; background: brown; top: 0; }
.cheese:after { right: 5px; top: 6px; width: 0;  height: 20px;  border-left: 5px solid transparent; border-right: 5px solid transparent; border-top: 0 solid #f6b93b; }

.bot_bun { margin-top: 52px; }

#burger:not(:checked) ~ [for="burger"] { 	 }
#burger:not(:checked) ~ [for="burger"], #burger:not(:checked) ~ [for="burger"] .cheese { -webkit-animation: jello-horizontal 0.7s both; animation: jello-horizontal 0.7s both; }

#burger:checked ~ [for="burger"] { filter: drop-shadow(0px 0px 9px rgba(0,0,0,0.3)); -webkit-filter: drop-shadow(0px 0px 9px rgba(0,0,0,0.3)); -moz-filter: drop-shadow(0px 0px 9px rgba(0,0,0,0.3));  }

#burger:checked ~ [for="burger"], #burger:checked ~ [for="burger"] .cheese { -webkit-animation: jello-vertical 0.7s both; animation: jello-vertical 0.7s both; }

